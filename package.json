{
  "name": "cap-admin-api",
  "version": "1.0.0",
  "description": "Cap Admin API",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/index.js",
    "dev": "nodemon src/main.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "format:code": "prettier --write src/**/*.{ts,js,graphql}",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "apqc": "yarn industries && ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/apqc.ts",
    "industries": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/industries.ts",
    "capabilities": "yarn industries && ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/capabilities.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.1",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.0.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "fast-csv": "^3.4.0",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "slug": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.15",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.11.2",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.5",
    "@types/jest": "^24.0.21",
    "@types/node": "^10.12.21",
    "@types/passport-local": "^1.0.33",
    "atob": ">=2.1.0",
    "deep-extend": ">=0.5.1",
    "extend": ">=3.0.2",
    "husky": "^3.0.9",
    "jest": "^24.5.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3"
  },
  "engines": {
    "node": "10.x"
  }
}
