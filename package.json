{
  "name": "cap-admin-api",
  "version": "1.0.0",
  "description": "Cap Admin API",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:old": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "dev": "nodemon src/main.ts",
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format:code": "prettier --write src/**/*.{ts,js,graphql}",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "apqc": "yarn processes",
    "industries": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/industries.ts",
    "processes": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/processes.ts",
    "capabilities": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/capabilities.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^6.10.10",
    "@nestjs/core": "^6.10.10",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.10.10",
    "@nestjs/swagger": "^4.0.9",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.10.10",
    "apollo-server-express": "^2.9.13",
    "cache-manager": "^2.10.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "fast-csv": "^3.4.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "slug": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.21",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.7.6",
    "@nestjs/testing": "^6.10.10",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.17",
    "@types/passport-local": "^1.0.33",
    "atob": ">=2.1.0",
    "deep-extend": ">=0.5.1",
    "extend": ">=3.0.2",
    "husky": "^3.0.9",
    "jest": "^24.5.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3"
  },
  "engines": {
    "node": "10.x"
  }
}
